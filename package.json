{
  "name": "project-management-dashboard",
  "version": "1.0.0",
  "description": "Advanced project management dashboard with AI insights, analytics, and comprehensive reporting",
  "homepage": "https://talhanust.github.io/Project_Management_Dashboard",
  "private": true,
  "dependencies": {
    "@mui/material": "^5.14.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/x-data-grid": "^6.11.0",
    "@mui/x-date-pickers": "^6.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.7.0",
    "date-fns": "^2.30.0",
    "web-vitals": "^2.1.4",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
    "docx": "^7.8.2",
    "pptxgenjs": "^3.11.0",
    "file-saver": "^2.0.5",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.7",
    "immer": "^10.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "gh-pages": "^5.0.0"
  },
  "keywords": [
    "react",
    "dashboard",
    "project-management",
    "analytics",
    "reporting",
    "ai-insights"
  ],
  "author": "Project Management Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/project-management-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/project-management-dashboard/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
